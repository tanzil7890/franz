(println "=== Escape Sequence Tests ===")
(println "")

// Test 1: Newline
(println "Test 1: Newline (\\n)")
test1 = "Line1\nLine2"
len1 = (length test1)
ok1 = (is len1 11)
(println (if ok1 {<- "PASS"} {<- "FAIL"}))

// Test 2: Tab
(println "Test 2: Tab (\\t)")
test2 = "Col1\tCol2"
