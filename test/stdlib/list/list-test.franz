(use "stdlib/list.franz" {
  (println "=== List Module Test Suite ===")
  (println "")

  // ===== reverse Tests =====
  (println "Testing reverse:")
  (println "  reverse([1,2,3]):" (reverse (list 1 2 3)))
  (println "  reverse([]):" (reverse (list)))
  (println "  reverse([1]):" (reverse (list 1)))
  (println "  reverse(['a','b','c']):" (reverse (list "a" "b" "c")))
  (println "")

  // ===== unique Tests =====
  (println "Testing unique:")
  (println "  unique([1,2,2,3,1]):" (unique (list 1 2 2 3 1)))
  (println "  unique([1,1,1]):" (unique (list 1 1 1)))
  (println "  unique([1,2,3]):" (unique (list 1 2 3)))
  (println "  unique([]):" (unique (list)))
  (println "  unique([5,3,5,1,3,1]):" (unique (list 5 3 5 1 3 1)))
  (println "")

  // ===== flatten Tests =====
  (println "Testing flatten:")
  (println "  flatten([[1,2],[3,4]]):" (flatten (list (list 1 2) (list 3 4))))
  (println "  flatten([[1],[2],[3]]):" (flatten (list (list 1) (list 2) (list 3))))
  (println "  flatten([1,[2,3],4]):" (flatten (list 1 (list 2 3) 4)))
  (println "  flatten([]):" (flatten (list)))
  (println "  flatten([[]]):" (flatten (list (list))))
  (println "")

  // ===== zip Tests =====
  (println "Testing zip:")
  (println "  zip([1,2,3],['a','b','c']):" (zip (list 1 2 3) (list "a" "b" "c")))
  (println "  zip([1,2],[10,20,30]):" (zip (list 1 2) (list 10 20 30)))
  (println "  zip([],[1,2]):" (zip (list) (list 1 2)))
  (println "  zip([1,2,3],[10]):" (zip (list 1 2 3) (list 10)))
  (println "")

  // ===== take Tests =====
  (println "Testing take:")
  (println "  take([1,2,3,4,5], 3):" (take (list 1 2 3 4 5) 3))
  (println "  take([1,2], 5):" (take (list 1 2) 5))
  (println "  take([1,2,3], 0):" (take (list 1 2 3) 0))
  (println "  take([], 3):" (take (list) 3))
  (println "  take([1,2,3,4,5], 5):" (take (list 1 2 3 4 5) 5))
  (println "")

  // ===== drop Tests =====
  (println "Testing drop:")
  (println "  drop([1,2,3,4,5], 2):" (drop (list 1 2 3 4 5) 2))
  (println "  drop([1,2], 5):" (drop (list 1 2) 5))
  (println "  drop([1,2,3], 0):" (drop (list 1 2 3) 0))
  (println "  drop([], 3):" (drop (list) 3))
  (println "  drop([1,2,3,4,5], 3):" (drop (list 1 2 3 4 5) 3))
  (println "")

  // ===== any Tests =====
  (println "Testing any:")
  nums1 = (list 1 2 3 4 5)
  (println "  any([1,2,3,4,5], >3):" (any nums1 {item index -> <- (greater_than item 3)}))
  (println "  any([1,2,3], >10):" (any (list 1 2 3) {item index -> <- (greater_than item 10)}))
  (println "  any([], >0):" (any (list) {item index -> <- (greater_than item 0)}))
  (println "  any([2,4,6], even?):" (any (list 2 4 6) {item index -> <- (is (remainder item 2) 0)}))
  (println "")

  // ===== all Tests =====
  (println "Testing all:")
  evens = (list 2 4 6 8)
  (println "  all([2,4,6,8], even?):" (all evens {item index -> <- (is (remainder item 2) 0)}))
  (println "  all([1,2,3], even?):" (all (list 1 2 3) {item index -> <- (is (remainder item 2) 0)}))
  (println "  all([], even?):" (all (list) {item index -> <- (is (remainder item 2) 0)}))
  (println "  all([1,3,5], odd?):" (all (list 1 3 5) {item index -> <- (is (remainder item 2) 1)}))
  (println "")

  // ===== partition Tests =====
  (println "Testing partition:")
  result1 = (partition (list 1 2 3 4 5 6) {item index -> <- (is (remainder item 2) 0)})
  (println "  partition([1,2,3,4,5,6], even?):" result1)
  result2 = (partition (list 1 2 3) {item index -> <- (greater_than item 10)})
  (println "  partition([1,2,3], >10):" result2)
  result3 = (partition (list) {item index -> <- 1})
  (println "  partition([], true):" result3)
  result4 = (partition (list 1 2 3 4 5) {item index -> <- (less_than item 3)})
  (println "  partition([1,2,3,4,5], <3):" result4)
  (println "")

  // ===== filled Tests =====
  (println "Testing filled:")
  (println "  filled(0, 5):" (filled 0 5))
  (println "  filled('x', 3):" (filled "x" 3))
  (println "  filled(42, 0):" (filled 42 0))
  (println "  filled([1,2], 2):" (filled (list 1 2) 2))
  (println "")

  // ===== chunk Tests =====
  (println "Testing chunk:")
  (println "  chunk([1,2,3,4,5,6], 2):" (chunk (list 1 2 3 4 5 6) 2))
  (println "  chunk([1,2,3,4,5], 2):" (chunk (list 1 2 3 4 5) 2))
  (println "  chunk([1,2,3], 5):" (chunk (list 1 2 3) 5))
  (println "  chunk([], 2):" (chunk (list) 2))
  (println "  chunk([1,2,3,4], 1):" (chunk (list 1 2 3 4) 1))
  (println "")

  // ===== sort Tests =====
  (println "Testing sort:")
  (println "  sort([3,1,4,1,5,9,2,6]):" (sort (list 3 1 4 1 5 9 2 6)))
  (println "  sort([5,4,3,2,1]):" (sort (list 5 4 3 2 1)))
  (println "  sort([1]):" (sort (list 1)))
  (println "  sort([]):" (sort (list)))
  (println "  sort([1,1,1]):" (sort (list 1 1 1)))
  (println "  sort([1,2,3,4,5]):" (sort (list 1 2 3 4 5)))
  (println "")

  (println "âœ“ All 12 list functions tested with 60+ test cases!")
})
