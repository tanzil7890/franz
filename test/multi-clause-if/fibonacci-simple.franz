// Fibonacci function using multi-clause if
fibonacci = {n ->
  <- (if
    (is n 0) {<- 0}
    (is n 1) {<- 1}
    {<- (add (fibonacci (subtract n 1)) (fibonacci (subtract n 2)))}
  )
}

(println "fib(0) = " (fibonacci 0))
(println "fib(1) = " (fibonacci 1))
(println "fib(2) = " (fibonacci 2))
(println "fib(3) = " (fibonacci 3))
(println "fib(4) = " (fibonacci 4))
(println "fib(5) = " (fibonacci 5))
(println "fib(6) = " (fibonacci 6))
