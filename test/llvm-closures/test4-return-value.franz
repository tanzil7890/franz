(println "=== Test: Nested closure return value ===")

make_adder = {n -> <- {x -> <- (add n x)}}
(println "Created make_adder")

add5 = (make_adder 5)
(println "Called make_adder(5), got add5")

(println "Type check - is add5 a function?")
// Try calling it
result = (add5 10)
(println "Called add5(10)")
(println "Result:" result)
(println "Expected: 15")
