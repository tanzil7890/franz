(println "=== Test 3: Nested closure (factory pattern) ===")
(println "Inner closure should capture ONLY 'n' from outer function")

// Nested function - outer returns a closure
make_adder = {n -> <- {x -> <- (add n x)}}
add5 = (make_adder 5)
result = (add5 10)
(println "make_adder(5)(10) =" result)
(println "Expected: 15")
