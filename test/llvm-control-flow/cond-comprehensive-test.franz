(println "")

(println "Part 1: Basic Cond Chains")
(println "--------------------------")

(println "Test 1: Two clauses with else")
test1_x = 15
test1_result = (cond
  ((greater_than test1_x 10) "big")
  (else "small"))
test1_pass = (is_string test1_result)
(println "Result:" test1_result "| Expected: big | Pass:" test1_pass)

(println "Test 2: Three clauses with else")
test2_x = 7
test2_result = (cond
  ((greater_than test2_x 10) "big")
  ((greater_than test2_x 5) "medium")
  (else "small"))
test2_pass = (is_string test2_result)
(println "Result:" test2_result "| Expected: medium | Pass:" test2_pass)

(println "Test 3: First clause matches")
test3_x = 12
test3_result = (cond
  ((greater_than test3_x 10) 100)
  ((greater_than test3_x 5) 50)
  (else 10))
test3_pass = (is test3_result 100)
(println "Result:" test3_result "| Expected: 100 | Pass:" test3_pass)

(println "Test 4: Second clause matches")
test4_x = 7
test4_result = (cond
  ((greater_than test4_x 10) 100)
  ((greater_than test4_x 5) 50)
  (else 10))
test4_pass = (is test4_result 50)
(println "Result:" test4_result "| Expected: 50 | Pass:" test4_pass)

(println "Test 5: Else clause matches")
test5_x = 2
test5_result = (cond
  ((greater_than test5_x 10) 100)
  ((greater_than test5_x 5) 50)
  (else 10))
test5_pass = (is test5_result 10)
(println "Result:" test5_result "| Expected: 10 | Pass:" test5_pass)

(println "Test 6: No else clause, no match (returns 0)")
test6_x = 2
test6_result = (cond
  ((greater_than test6_x 10) 100)
  ((greater_than test6_x 5) 50))
test6_pass = (is test6_result 0)
(println "Result:" test6_result "| Expected: 0 | Pass:" test6_pass)

(println "Test 7: Single clause with else")
