test18_result = (cond
  ((greater_than test18_x 50) (multiply test18_x 2))
  (else (divide test18_x 2)))
test18_pass = (is test18_result 200)
(println "Result:" test18_result "| Expected: 200 | Pass:" test18_pass)

(println "Test 19: Multiple arithmetic operations")
