(println "Test 17: Arithmetic in test")
test17_a = 5
test17_b = 10
test17_result = (cond
  ((greater_than (add test17_a test17_b) 10) "big sum")
  (else "small sum"))
test17_pass = (is_string test17_result)
(println "Result:" test17_result "| Expected: big sum | Pass:" test17_pass)

(println "Test 18: Arithmetic in result")
