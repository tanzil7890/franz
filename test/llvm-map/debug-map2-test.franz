(println "=== Debug Map2 Test ===")

list1 = [1, 2, 3]
list2 = [10, 20, 30]

add_both = {a b i -> <- (add a b)}
result = (map2 list1 list2 add_both)

(println "Result is list?" (is_list result))
(println "Result length:" (length result))
(println "First element:" (head result))
(println "Second element:" (nth result 1))
(println "Third element:" (nth result 2))
