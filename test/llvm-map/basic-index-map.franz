// Test 2: Map with index usage
(println "Test 2: Map using index")
nums2 = [10, 20, 30]
add_index = {x i -> <- (add x i)}
with_index = (map nums2 add_index)
(println "Input:" nums2)
(println "With index added:" with_index)
(println "PASS: Map with index works")
(println "")