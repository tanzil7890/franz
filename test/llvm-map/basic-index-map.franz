
// Test 1: Basic map with simple transformation
(println "Test 1: Basic map - double numbers")
nums1 = [1, 2, 3, 4, 5]
double = {x i -> <- (multiply x 2)}
doubled = (map nums1 double)
(println "Input:" nums1)
(println "Doubled:" doubled)
(println "PASS: Basic map works")
