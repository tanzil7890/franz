is_adult = (greater_than age 17)
not_senior = (less_than age 65)
is_valid_age = (and is_adult not_senior)
(println "Is valid age:" is_valid_age)
(println "")

// Test 5.2: Weekend check (Saturday OR Sunday)
(println "Test 5.2: Weekend check (day=6 for Saturday)")
