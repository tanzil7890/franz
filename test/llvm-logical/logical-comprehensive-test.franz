// ============================================================================
// Category 4: Complex Boolean Expressions
// ============================================================================
(println "Category 4: Complex Boolean Expressions")
(println "")

// Test 4.1: NOT with AND
(println "Test 4.1: (not (and 1 0)) should be 1")
result_4_1 = (not (and 1 0))
(println "Result:" result_4_1)
(println "")

// Test 4.2: NOT with OR
(println "Test 4.2: (not (or 0 0)) should be 1")
result_4_2 = (not (or 0 0))
(println "Result:" result_4_2)
(println "")

// Test 4.3: AND with OR (nested)
(println "Test 4.3: (and 1 (or 0 1)) should be 1")
result_4_3 = (and 1 (or 0 1))
(println "Result:" result_4_3)
(println "")

// Test 4.4: OR with AND (nested)
(println "Test 4.4: (or 0 (and 1 1)) should be 1")
result_4_4 = (or 0 (and 1 1))
(println "Result:" result_4_4)
(println "")

// Test 4.5: De Morgan's Law - NOT (A AND B) = (NOT A) OR (NOT B)
(println "Test 4.5: De Morgan 1 - (not (and 1 1)) = (or (not 1) (not 1))")
left_4_5 = (not (and 1 1))
right_4_5 = (or (not 1) (not 1))
(println "Left:" left_4_5 "Right:" right_4_5)
(println "")

// Test 4.6: De Morgan's Law - NOT (A OR B) = (NOT A) AND (NOT B)
(println "Test 4.6: De Morgan 2 - (not (or 0 0)) = (and (not 0) (not 0))")
left_4_6 = (not (or 0 0))
right_4_6 = (and (not 0) (not 0))
(println "Left:" left_4_6 "Right:" right_4_6)
(println "")

