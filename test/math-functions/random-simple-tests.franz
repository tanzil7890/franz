(println "=== Enhanced Random Functions Test Suite ===")
(println "")

(println "TEST 1: random_int - Returns integer in range")
test1_r = (random_int 10)
test1_pass = (and (greater_than test1_r -1) (less_than test1_r 10))
(println "  random_int(10) = " test1_r)
(println "  PASS:" test1_pass)

(println "")
(println "TEST 2: random_int - Multiple calls vary")
test2_a = (random_int 100)
test2_b = (random_int 100)
test2_c = (random_int 100)
test2_pass = (is (and (is test2_a test2_b) (is test2_b test2_c)) 0)
(println "  Values: " test2_a test2_b test2_c)
(println "  PASS:" test2_pass)

(println "")
(println "TEST 3: random_range - Returns float in range")
test3_r = (random_range 5 15)
test3_pass = (and (greater_than test3_r 4.99) (less_than test3_r 15.01))
(println "  random_range(5, 15) = " test3_r)
(println "  PASS:" test3_pass)

(println "")
(println "TEST 4: random_range - Negative range works")
test4_r = (random_range -10 -5)
test4_pass = (and (greater_than test4_r -10.01) (less_than test4_r -4.99))
(println "  random_range(-10, -5) = " test4_r)
(println "  PASS:" test4_pass)

(println "")
(println "TEST 5: random_seed - Reproducible sequence")
(random_seed 42)
test5_a = (random)
test5_b = (random)
(random_seed 42)
test5_c = (random)
test5_d = (random)
test5_pass = (and (is test5_a test5_c) (is test5_b test5_d))
(println "  Seed 42 first: " test5_a test5_b)
(println "  Seed 42 again: " test5_c test5_d)
(println "  PASS:" test5_pass)

(println "")
(println "TEST 6: random_seed - Different seeds differ")
(random_seed 100)
test6_a = (random)
(random_seed 200)
test6_b = (random)
test6_pass = (is (is test6_a test6_b) 0)
(println "  Seed 100: " test6_a)
(println "  Seed 200: " test6_b)
(println "  PASS:" test6_pass)

(println "")
