(println "=== Franz Math Functions Edge Case Test Suite ===")
(println "")

(println "TEST 1: remainder - Very large numbers")
result1 = (remainder 999999 7)
(println "  remainder(999999, 7) = " result1)
(println "  Expected: 0")
(println "  PASS:" (is result1 0))

(println "")
(println "TEST 2: remainder - Small divisor")
result2 = (remainder 100 1)
(println "  remainder(100, 1) = " result2)
(println "  Expected: 0")
(println "  PASS:" (is result2 0))

(println "")
(println "TEST 3: remainder - Equal numbers")
result3 = (remainder 42 42)
(println "  remainder(42, 42) = " result3)
(println "  Expected: 0")
(println "  PASS:" (is result3 0))

(println "")
(println "TEST 4: remainder - Dividend smaller than divisor")
result4 = (remainder 3 10)
(println "  remainder(3, 10) = " result4)
(println "  Expected: 3")
(println "  PASS:" (is result4 3))

(println "")
(println "TEST 5: remainder - Very small float remainder")
result5 = (remainder 10.1 5.0)
(println "  remainder(10.1, 5.0) = " result5)
(println "  Expected: ~0.1")
expected5 = 0.1
diff5 = (subtract result5 expected5)
abs_diff5 = (multiply diff5 (if (less_than diff5 0) {<- -1} {<- 1}))
pass5 = (less_than abs_diff5 0.01)
(println "  PASS:" pass5)

(println "")
(println "TEST 6: power - Large base, small exponent")
result6 = (power 1000 2)
(println "  power(1000, 2) = " result6)
(println "  Expected: 1000000")
(println "  PASS:" (is result6 1000000))

(println "")
(println "TEST 7: power - Square root (fractional exponent)")
result7 = (power 16 0.5)
(println "  power(16, 0.5) = " result7)
(println "  Expected: 4.0")
expected7 = 4.0
diff7 = (subtract result7 expected7)
abs_diff7 = (multiply diff7 (if (less_than diff7 0) {<- -1} {<- 1}))
pass7 = (less_than abs_diff7 0.0001)
(println "  PASS:" pass7)

(println "")
(println "TEST 8: power - Cube root (fractional exponent)")
result8 = (power 27 0.3333333333)
(println "  power(27, 1/3) = " result8)
(println "  Expected: ~3.0")
expected8 = 3.0
diff8 = (subtract result8 expected8)
abs_diff8 = (multiply diff8 (if (less_than diff8 0) {<- -1} {<- 1}))
pass8 = (less_than abs_diff8 0.001)
(println "  PASS:" pass8)

(println "")
(println "TEST 9: power - Very small fractional exponent")
result9 = (power 100 0.1)
(println "  power(100, 0.1) = " result9)
(println "  Expected: ~1.584893")
expected9 = 1.584893
diff9 = (subtract result9 expected9)
abs_diff9 = (multiply diff9 (if (less_than diff9 0) {<- -1} {<- 1}))
pass9 = (less_than abs_diff9 0.001)
(println "  PASS:" pass9)

(println "")
(println "TEST 10: power - Base is 1 with various exponents")
result10a = (power 1 0)
result10b = (power 1 1)
result10c = (power 1 100)
(println "  power(1, 0) = " result10a)
(println "  power(1, 1) = " result10b)
(println "  power(1, 100) = " result10c)
pass10 = (and (is result10a 1) (and (is result10b 1) (is result10c 1)))
(println "  PASS (all equal 1):" pass10)

(println "")
(println "TEST 11: power - Exponent is 1 with various bases")
result11a = (power 0 1)
result11b = (power 5 1)
result11c = (power 999 1)
(println "  power(0, 1) = " result11a)
(println "  power(5, 1) = " result11b)
(println "  power(999, 1) = " result11c)
pass11 = (and (is result11a 0) (and (is result11b 5) (is result11c 999)))
(println "  PASS (all equal base):" pass11)

(println "")
(println "TEST 12: power - Negative base with even exponent")
result12 = (power -2 4)
(println "  power(-2, 4) = " result12)
(println "  Expected: 16")
(println "  PASS:" (is result12 16))

(println "")
(println "TEST 13: power - Negative base with odd exponent")
result13 = (power -2 3)
(println "  power(-2, 3) = " result13)
(println "  Expected: -8")
(println "  PASS:" (is result13 -8))

(println "")
(println "TEST 14: random - Multiple calls return values in range")
r1 = (random)
r2 = (random)
r3 = (random)
r4 = (random)
r5 = (random)
(println "  Random values: " r1 r2 r3 r4 r5)
in_range1 = (and (greater_than r1 -0.01) (less_than r1 1.01))
in_range2 = (and (greater_than r2 -0.01) (less_than r2 1.01))
in_range3 = (and (greater_than r3 -0.01) (less_than r3 1.01))
in_range4 = (and (greater_than r4 -0.01) (less_than r4 1.01))
in_range5 = (and (greater_than r5 -0.01) (less_than r5 1.01))
all_in_range = (and in_range1 (and in_range2 (and in_range3 (and in_range4 in_range5))))
(println "  All in range [0, 1]? " all_in_range)
(println "  PASS:" all_in_range)

(println "")
(println "=== Summary ===")
(println "All 14 edge case tests completed!")
(println "Check each PASS value above (1 = passed, 0 = failed)")
