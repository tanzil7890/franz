(println "=== Franz Math Functions Comprehensive Test Suite ===")
(println "")

(println "TEST 1: remainder - Basic integer remainder")
result1 = (remainder 10 3)
(println "  remainder(10, 3) = " result1)
(println "  Expected: 1")
(println "  PASS:" (is result1 1))

(println "")
(println "TEST 2: remainder - Zero dividend")
result2 = (remainder 0 5)
(println "  remainder(0, 5) = " result2)
(println "  Expected: 0")
(println "  PASS:" (is result2 0))

(println "")
(println "TEST 3: remainder - Remainder equals divisor minus 1")
