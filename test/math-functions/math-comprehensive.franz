(println "  remainder(17, 5) = " result3)
(println "  Expected: 2")
(println "  PASS:" (is result3 2))

(println "")
(println "TEST 4: remainder - Exact division (no remainder)")
result4 = (remainder 15 5)
(println "  remainder(15, 5) = " result4)
(println "  Expected: 0")
(println "  PASS:" (is result4 0))

(println "")
(println "TEST 5: remainder - Negative dividend")
