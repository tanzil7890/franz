(println "  remainder(-10, 3) = " result5)
(println "  Expected: -1")
(println "  PASS:" (is result5 -1))

(println "")
(println "TEST 6: remainder - Negative divisor")
result6 = (remainder 10 -3)
(println "  remainder(10, -3) = " result6)
(println "  Expected: 1")
(println "  PASS:" (is result6 1))

(println "")
(println "TEST 7: remainder - Float remainder")
result7 = (remainder 10.5 3.0)
(println "  remainder(10.5, 3.0) = " result7)
(println "  Expected: ~1.5")
expected7 = 1.5
diff7 = (subtract result7 expected7)
pass7 = (less_than (if (less_than diff7 0) {<- (multiply diff7 -1)} {<- diff7}) 0.0001)
(println "  PASS:" pass7)

