(println "")
(println "TEST 20: random - Multiple calls return different values")
r2 = (random)
r3 = (random)
r4 = (random)
(println "  random() call 1: " r2)
(println "  random() call 2: " r3)
(println "  random() call 3: " r4)
all_in_range = (and (and (greater_than r2 -0.01) (less_than r2 1.01)) (and (and (greater_than r3 -0.01) (less_than r3 1.01)) (and (greater_than r4 -0.01) (less_than r4 1.01))))
(println "  All in range? " all_in_range)
not_all_same = (is (and (is r2 r3) (is r3 r4)) 0)
(println "  Not all same? " not_all_same)
(println "  PASS:" (and all_in_range not_all_same))

